<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.msc.Mozi" alias="Mozi"/>
 		<typeAlias type="com.msc.Vevo" alias="Vevo"/>
 		<typeAlias type="com.msc.VM" alias="VM"/>
 	</typeAliases>
 	<environments default="development">
 	<environment id="development">
 		<transactionManager type="JDBC"/>
 			<dataSource type="POOLED">
 				<property name="driver" value="com.mysql.cj.jdbc.Driver"/>
 				<property name="url" value="jdbc:mysql://localhost:3306/mozi"/>
 				<property name="username" value="root"/>
 				<property name="password" value="bruh"/>
 			</dataSource>
 	</environment>
 </environments>
 <mappers>
 	<mapper resource="com/msc/MoziMapper.xml"/>
 	<mapper resource="com/msc/VevoMapper.xml"/>
 	<mapper resource="com/msc/VMMapper.xml"/>
 </mappers>
</configuration>

